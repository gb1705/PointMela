                                android:id="@+id/spitem"
                                android:layout_marginTop="20dp"
                                android:spinnerMode="dropdown"></Spinner>
                        </LinearLayout>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.010"
                        android:orientation="horizontal"
                        ></LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.0030"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/txt"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="As per Medical Council I am part of"
                            android:textColor="#000000"
                            android:visibility="gone"
                            android:layout_gravity="center_horizontal"
                            android:gravity="center"
                            android:textSize="15sp"
                            android:textAlignment="center"
                            android:layout_marginTop="0dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp" />

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.23"
                        android:orientation="horizontal"
                        ></LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.0050"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/txt1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="following District"
                            android:textColor="#000000"
                            android:layout_gravity="center_horizontal"
                            android:gravity="center"
                            android:textSize="15sp"
                            android:visibility="gone"
                            />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.010"
                        android:orientation="horizontal"
                        ></LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1.0010"
